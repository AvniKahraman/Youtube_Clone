<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/imagetube"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Youtube"
        tools:targetApi="31">
        <activity
            android:name=".Bookstore"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.youtube.api.key"
            android:value="AIzaSyA6sVY1sf3eUimFVVqWJWKvTfVdBqxdDKI" />

        <activity
            android:name=".Subscriptions"
            android:exported="false" />
    </application>

</manifest>